; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="SCP-001: Memory Recovery Protocol"
run/main_scene="res://Scenes/OSRoot/OSRoot.tscn"
run/print_header=false
config/features=PackedStringArray("4.3", "Forward Plus")
run/size/viewport_height=720

[autoload]

REFS="*res://Autoload/enums/NODE_REFS.gd"
BUILDING_TYPE="*res://Autoload/enums/BUILDING_TYPE.gd"
RESOURCE="*res://Autoload/enums/RESOURCE.gd"
SCP="*res://Autoload/enums/SCP.gd"
SVGS="*res://Autoload/enums/SVGS.gd"
COLORS="*res://Autoload/enums/COLORS.gd"
ACTION="*res://Autoload/enums/ACTIONS.gd"
CACHE="*res://Autoload/Cache.gd"
GBL="*res://Autoload/Global.gd"
FS="*res://Autoload/FileSys.gd"
B="*res://Autoload/Buildables.gd"
U="*res://Autoload/Utility.gd"
C="*res://Autoload/Containment.gd"
COLOR_REF="*res://Autoload/ColorRef.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/size/always_on_top=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/stretch/scale_mode="integer"
window/dpi/allow_hidpi=false
window/main_scene="res://Scenes/Computer/Computer.tscn"
